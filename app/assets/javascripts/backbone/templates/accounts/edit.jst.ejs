<h1>Edit account</h1>

<form id="edit-account" name="account">
  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value="<%= name %>" >
  </div>

  <div class="field">
    <label for="number"> number:</label>
    <input type="text" name="number" id="number" value="<%= number %>" >
  </div>

  <div class="field">
    <label for="branch"> branch:</label>
    <input type="text" name="branch" id="branch" value="<%= branch %>" >
  </div>

  <div class="field">
    <label for="balance"> balance:</label>
    <input type="text" name="balance" id="balance" value="<%= balance %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Update Account" />
  </div>

</form>

<a href="#/index">Back</a>